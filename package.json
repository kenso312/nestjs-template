    "@nestjs/platform-fastify": "^8.0.6",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "axios": "^0.24.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "fastify": "^3.22.0",
    "joi": "^17.4.2",
    "mysql2": "^2.3.0",
    "nestjs-i18n": "^8.1.6",
    "reflect-metadata": "^0.1.13",
