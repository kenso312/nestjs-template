  "scripts": {
    "build": "nest build",
    "start": "yarn build && yarn start:prod",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "dev": "yarn start:dev",
    "prod": "yarn start:prod",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/database/typeorm.config.ts",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:create": "npm run typeorm:cli -- migration:create --name",
    "migration:make": "npm run typeorm:cli -- migration:generate --pretty --name",
    "lint": "eslint . --ext ts --ext js --fix",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest",
    "test:e2e": "jest --config ./test/jest.e2e.config.js --passWithNoTests",
    "make": "node ./.template/init.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
