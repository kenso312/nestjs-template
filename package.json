    "eslint-plugin-unused-imports": "^1.1.4",
    "husky": "^7.0.2",
    "jest": "27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
